#include<stdio.h>
int main(){
    int a, b, c;
    printf("Enter Three Number\n");
    scanf("%d %d %d",&a, &b, &c);
    if (a>=b && a>=c)
    {printf("This n1 is greatest integer =%d",a);  }
    else if (b>=a && b>=c)
    {printf("This n2 is greatest integer =%d",b);}
    else if(c>=b && c>=b)
    {printf("This n3 is greatest integer =%d",c); }
    return 0;
    }
