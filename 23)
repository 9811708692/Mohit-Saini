#include<stdio.h>

int main(){
    int num, rem, sum;
    for (int i = 1; i< 500 ; i++)
    {
        num=i;
        sum=0;
        while (num)
        {
            rem = num %10;
            sum=sum+(rem*rem*rem);
            num=num/10;
        }
        if (i==sum)
        {
            printf("%d Is A Armstrong Number \n",i);
        }   
    }   
  return 0;}
